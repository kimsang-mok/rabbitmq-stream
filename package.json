{
  "name": "rabbitmq-stream",
  "version": "0.1.3",
  "description": "A declarative RabbitMQ messaging framework for Node.js inspired by Spring Cloud Stream",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "ts-node -r tsconfig-paths/register src/index.ts",
    "build": "tsc && tsc-alias",
    "watch": "tsc -w",
    "start": "node dist/index.js"
  },
  "keywords": [
    "amqp",
    "rabbitmq",
    "messaging",
    "framework",
    "nodejs",
    "event-driven",
    "spring-cloud-stream"
  ],
  "author": "Kimsang Mok",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kimsang-mok/rabbitmq-stream"
  },
  "bugs": {
    "url": "https://github.com/kimsang-mok/rabbitmq-stream/issues"
  },
  "homepage": "https://github.com/kimsang-mok/rabbitmq-stream#readme",
  "dependencies": {
    "amqplib": "^0.10.7",
    "reflect-metadata": "^0.2.2",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.7",
    "@types/node": "^22.15.2",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.15",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  }
}
